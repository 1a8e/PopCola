<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_welcome"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="in.discreetmusings.popcola.Welcome"
    tools:showIn="@layout/activity_welcome">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView_theater"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_theater"
        android:layout_alignParentStart="true"
        android:id="@+id/textView_partner" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_partner"
        android:layout_alignParentStart="true"
        android:id="@+id/textView_region" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_partner"
        android:layout_toEndOf="@+id/textView_partner"
        android:id="@+id/textView_city" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_screen"
        android:layout_below="@+id/textView14"
        android:layout_toStartOf="@+id/textView_show"
        android:layout_alignEnd="@+id/textView_movie" />

    <TextView
        android:text="Movie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView16"
        android:layout_below="@+id/textView_region"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_movie"
        android:layout_alignBaseline="@+id/textView_show"
        android:layout_alignBottom="@+id/textView_show"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:layout_below="@+id/textView_movie"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_seat"
        android:layout_below="@+id/textView15"
        android:layout_toEndOf="@+id/textView_screen" />

    <TextView
        android:text="Seat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:layout_below="@+id/textView_show"
        android:layout_toEndOf="@+id/textView_screen" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_show"
        android:layout_below="@+id/textView16"
        android:layout_toEndOf="@+id/textView_movie" />

    <TextView
        android:text="Show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_city"
        android:layout_toEndOf="@+id/textView_region"
        android:id="@+id/textView17" />
</RelativeLayout>
